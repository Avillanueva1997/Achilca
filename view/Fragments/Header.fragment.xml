<core:FragmentDefinition
    xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:tnt="sap.tnt"
	xmlns:core="sap.ui.core">   
    
    <tnt:ToolHeader>
        <Button icon="sap-icon://home" type="Transparent" press="onHomePress" tooltip="Inicio">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <Image src="assets/images/favicon_n2.png" width="2.5rem">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Image>
        <ToolbarSpacer />
        <Button icon="sap-icon://group" type="Transparent" tooltip="Equipo" press="onTeam">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <Button icon="sap-icon://business-objects-experience" type="Transparent" tooltip="Indicadores" press="onKpi">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <Button icon="sap-icon://log" type="Transparent" tooltip="Salir" press="onLogout">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
	</tnt:ToolHeader>
    
</core:FragmentDefinition>
