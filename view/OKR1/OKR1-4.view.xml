<mvc:View
    controllerName="sap.ui.app01.controller.OKR1.OKR1-4"
    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:unified="sap.ui.unified"
    xmlns:t="sap.ui.table"
    xmlns="sap.m"
    xmlns:viz="sap.viz.ui5.controls" >
    <Page
      title="Cantidad de Accidentes"
      id="SIMA04"
      showNavButton="true"
      navButtonPress="onNavBack">
      <headerContent>
        <Button press="onAddAccidente" tooltip="Accidente" icon="sap-icon://wounds-doc"/>
        <Button press="onAddIncidente" tooltip="Incidente" icon="sap-icon://incident"/>
      </headerContent>
    <content>
    <l:VerticalLayout width="100%">
      <l:Grid id="gridHeader" defaultSpan="XL12 L12 M12 S12" class="sapUiTinyMarginTop">
        <l:content>
            <viz:VizFrame id="idChart1" width="100%"></viz:VizFrame>
        </l:content>
      </l:Grid>
    </l:VerticalLayout>
    <HBox height="100%" alignItems="End" class="hboxContentButtonMiddle">
      <Avatar
      src="sap-icon://activity-2"
      displaySize="S"
      backgroundColor="Accent6"
      press="openFragmentPASIMA"/>
		</HBox>
    </content>
    </Page>
</mvc:View>